{
  "name": "confere.js",
  "version": "0.5.7",
  "description": "A simple and powefull promise based javascript validation library with extras",
  "main": "dist/confere.js",
  "scripts": {
    "build": "cross-env WEBPACK_ENV=build webpack --progress --colors & npm run build-dev",
    "build-dev": "cross-env WEBPACK_ENV=dev webpack --progress --colors",
    "dev": "cross-env WEBPACK_ENV=dev webpack --progress --colors --watch",
    "docs-dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.docs.config.js --open --hot",
    "docs-build": "cross-env NODE_ENV=production webpack --progress --config ./webpack.docs.config.js --hide-modules",
    "lint-fix": "eslint --fix src/*",
    "test": "mocha --compilers js:babel-core/register --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isneezy/confere.js.git"
  },
  "bugs": {
    "url": "https://github.com/isneezy/confere.js/issues"
  },
  "homepage": "https://github.com/isneezy/confere.js#readme",
  "keywords": [
    "confere",
    "form-validation",
    "validation",
    "input",
    "validate",
    "data-valdiation",
    "input-validation",
    "validation-library"
  ],
  "author": "Ivan Vilanculo <vilanculoivan@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "3.4.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "expect.js": "^0.3.1",
    "file-loader": "^0.11.2",
    "highlight.js": "^9.12.0",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "10.2.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.7.1"
  }
}
